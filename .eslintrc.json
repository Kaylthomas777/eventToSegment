{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": "error"
  }
}
